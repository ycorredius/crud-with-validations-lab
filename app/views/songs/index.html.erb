<h1>Songs</h1>
<div>link_to "Create new songs" new_song_path</div>
<table>
    <thead>
        <th>Title:</th>
        <th>Artist Name:</th>
        <th>Release Year:</th>
        <th>Genre:</th>
    </thead>
    <tbody>
        <% @songs.each |song| %>
        <th><%= song.title %></th>
        <th><%= song.artist_name %></th>
        <th><%= song.release_year %></th>
        <th><%= sogn.genre %></th>
    </tbody>
</table>